FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../system/parameters"

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform $inletVelocity;  // Usa a vari√°vel do parameters
    }

    outlet
    {
        type            zeroGradient;
    }

    walls
    {
        type            noSlip;
    }


    frontAndBack
    {
        type            empty;
    }
}

